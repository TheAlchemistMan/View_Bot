import webbrowser as b
import time

views = int(input("How much views do you want : "))

URL = 'INPUT YOUR URL HERE'



number = range(1,100) //Edit range as you wish
i = 0
for count in number:
    while i < views:
        b.open(URL)
        i += 1


        print(str(count) +" Opening " + URL)
        time.sleep(5) //Seconds to wait
        break




input("Press any key to continue...")



